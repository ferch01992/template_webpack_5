{
  "compilerOptions": {
    "jsx": "react",
    "baseUrl": "./",
    "paths": {
      "Components/*": ["src/components/*"],
      "Actions/*": ["src/store/actions/*"],
      "Conteiners/*": ["src/containers/*"],
      "Images/*": ["src/assets/images/*"],
      "Module/*": ["src/module/*"],
      "Reducers/*": ["src/store/reducers/*"],
      "StoreConstans/*": ["src/store/constans/*"]
    }
  },
  "excluir": [
    "node_modules",
    "**/node_modules/*",
    "public",
    "**/public/*",
    "webpack",
    "**/webpack/*",
    ".husky",
    "**/.husky/*"
  ]
}
